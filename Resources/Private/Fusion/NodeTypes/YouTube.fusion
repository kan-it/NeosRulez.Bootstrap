prototype(NeosRulez.Bootstrap:YouTube) < prototype(Neos.Neos:Plugin) {
    package = 'NeosRulez.Bootstrap'
    controller = 'Frontend'
    action = 'youTube'
    link = ${q(node).property('link')}
    attributes.class = 'embed-responsive embed-responsive-16by9'
    @cache {
        mode = 'uncached'
        context {
            1 = 'node'
            2 = 'documentNode'
        }
    }
}