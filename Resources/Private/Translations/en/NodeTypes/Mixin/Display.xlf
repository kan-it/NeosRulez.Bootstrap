<?xml version="1.0" encoding="UTF-8"?>
<xliff version="1.2" xmlns="urn:oasis:names:tc:xliff:document:1.2">
    <file original="" product-name="NeosRulez.Bootstrap" source-language="en" datatype="plaintext">
        <body>
            <trans-unit id="properties.display" xml:space="preserve">
				<source>Type of representation</source>
			</trans-unit>
            <trans-unit id="properties.display.selectBoxEditor.values.none" xml:space="preserve">
                <source>d-none: The item is completely removed.</source>
            </trans-unit>
            <trans-unit id="properties.display.selectBoxEditor.values.inline" xml:space="preserve">
                <source>d-inline: Displays an element as an inline element. All height and width properties have no effect.</source>
            </trans-unit>
            <trans-unit id="properties.display.selectBoxEditor.values.inline-block" xml:space="preserve">
                <source>d-inline-block: Displays an element as an inline-level block container. The element itself is formatted as an inline element, but you can apply height and width values.</source>
            </trans-unit>
            <trans-unit id="properties.display.selectBoxEditor.values.block" xml:space="preserve">
                <source>d-block: Displays an element as a block element. It starts on a new line and takes up the entire width.</source>
            </trans-unit>
            <trans-unit id="properties.display.selectBoxEditor.values.grid" xml:space="preserve">
                <source>d-grid: Displays an item as a block-level Grid container.</source>
            </trans-unit>
            <trans-unit id="properties.display.selectBoxEditor.values.table" xml:space="preserve">
                <source>d-table: The element behaves like a table element.</source>
            </trans-unit>
            <trans-unit id="properties.display.selectBoxEditor.values.table-cell" xml:space="preserve">
                <source>d-table-cell: The element behaves like a td element.</source>
            </trans-unit>
            <trans-unit id="properties.display.selectBoxEditor.values.table-row" xml:space="preserve">
                <source>d-table-row: The element behaves like a tr element.</source>
            </trans-unit>
            <trans-unit id="properties.display.selectBoxEditor.values.flex" xml:space="preserve">
                <source>d-flex: Displays an element as a flexible block-level container.</source>
            </trans-unit>
            <trans-unit id="properties.display.selectBoxEditor.values.inline-flex" xml:space="preserve">
                <source>d-inline-flex: Displays an item as an inline-level flex container.</source>
            </trans-unit>
        </body>
    </file>
</xliff>
