  "type": "neos-package",
  "name": "neosrulez/bootstrap",
  "require": {
    "neos/neos": "^7.0 || ^8.0 || ^8.1",
    "neos/nodetypes": "*",
    "shel/neos-colorpicker": "^1.4",
    "neosrulez/bootstrap-formbuilder": "*",
